	<select id="selectListVO"
		resultType="com.entity.vo.WuyefeiVO" >
		SELECT * FROM wuyefei  wuyefei         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.WuyefeiVO" >
		SELECT  wuyefei.* FROM wuyefei  wuyefei 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.WuyefeiView" >

		SELECT  wuyefei.* FROM wuyefei  wuyefei 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.WuyefeiView" >
		SELECT * FROM wuyefei  wuyefei <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
</mapper>