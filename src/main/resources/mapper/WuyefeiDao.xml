<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.WuyefeiDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.WuyefeiEntity" id="wuyefeiMap">
        <result property="jiaofeimingcheng" column="jiaofeimingcheng"/>
        <result property="yuefen" column="yuefen"/>
        <result property="jiaonajine" column="jiaonajine"/>
        <result property="dengjishijian" column="dengjishijian"/>
        <result property="zhuhuzhanghao" column="zhuhuzhanghao"/>
        <result property="zhuhuxingming" column="zhuhuxingming"/>
